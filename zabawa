-- ≈Åadowanie Fluent i dodatk√≥w
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

-- Tworzenie okna
local Window = Fluent:CreateWindow({
    Title = "Dziewczynkomator",
    SubTitle = "by PreXezzz",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

-- Tworzenie zak≈Çadki
local Tabs = {
    Main = Window:AddTab({ Title = "G≈Ç√≥wne", Icon = "home" }),
    Settings = Window:AddTab({ Title = "Ustawienia", Icon = "settings" })
}

-- Powiadomienie przy uruchomieniu
Fluent:Notify({
    Title = "üöÄ Skrypt Za≈Çadowany",
    Content = "Witaj w Dziewczynkomatorze by PreXezzz!",
    Duration = 8
})

-- Przycisk teleportacji wszystkich graczy do mnie
Tabs.Main:AddButton({
    Title = "Teleportuj Wszystkich do Mnie üåç",
    Description = "Teleportuje wszystkich graczy do Twojej pozycji",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local rootPart = character:WaitForChild("HumanoidRootPart")
        
        if rootPart then
            for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                if otherPlayer ~= player and otherPlayer.Character then
                    local targetRoot = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
                    local targetHumanoid = otherPlayer.Character:FindFirstChild("Humanoid")
                    if targetRoot and targetHumanoid then
                        task.spawn(function()
                            targetRoot.Anchored = true
                            targetHumanoid.WalkSpeed = 0
                            targetRoot.CFrame = rootPart.CFrame + Vector3.new(math.random(-5, 5), 0, math.random(-5, 5))
                            wait(0.1)
                            targetRoot.Anchored = false
                            targetHumanoid.WalkSpeed = 16
                        end)
                    end
                end
            end
            Fluent:Notify({
                Title = "‚úÖ Sukces",
                Content = "Wszyscy gracze teleportowani do Ciebie!",
                Duration = 4
            })
        else
            Fluent:Notify({
                Title = "‚ùå B≈ÇƒÖd",
                Content = "Nie znaleziono Twojej postaci!",
                Duration = 4
            })
        end
    end
})

-- Przycisk dodajƒÖcy napis "YouTuberüî¥" nad g≈ÇowƒÖ
Tabs.Main:AddButton({
    Title = "Dodaj Napis YouTuberüî¥ üìπ",
    Description = "Dodaje napis nad TwojƒÖ g≈ÇowƒÖ widoczny dla wszystkich",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local head = character:WaitForChild("Head")
        
        local existingLabel = character:FindFirstChild("YouTuberLabel")
        if existingLabel then
            existingLabel:Destroy()
        end
        
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "YouTuberLabel"
        billboard.Adornee = head
        billboard.Size = UDim2.new(0, 100, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = game.ReplicatedStorage
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = "YouTuberüî¥"
        label.TextColor3 = Color3.fromRGB(255, 0, 0)
        label.TextScaled = true
        label.Font = Enum.Font.SourceSansBold
        label.Parent = billboard
        
        task.spawn(function()
            wait(0.1)
            billboard.Parent = character
        end)
        
        Fluent:Notify({
            Title = "‚úÖ Sukces",
            Content = "Napis YouTuberüî¥ dodany nad TwojƒÖ g≈ÇowƒÖ!",
            Duration = 4
        })
    end
})

-- Przycisk dodajƒÖcy i respawnuƒÖcy Part na mojej pozycji
local partInstance = nil
Tabs.Main:AddButton({
    Title = "Respawn Part na Mojej Pozycji üü•",
    Description = "Tworzy i respawnuje widoczny Part na Twojej pozycji",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local rootPart = character:WaitForChild("HumanoidRootPart")
        
        if rootPart then
            -- Usuwamy istniejƒÖcy Part, je≈õli istnieje
            if partInstance then
                partInstance:Destroy()
            end
            
            -- Tworzymy nowy Part w Workspace
            partInstance = Instance.new("Part")
            partInstance.Name = "DziewczynkomatorPart"
            partInstance.Size = Vector3.new(5, 5, 5)
            partInstance.Position = rootPart.Position + Vector3.new(0, 5, 0) -- Nad graczem
            partInstance.BrickColor = BrickColor.new("Bright red")
            partInstance.Material = Enum.Material.Neon
            partInstance.Anchored = true
            partInstance.CanCollide = false
            partInstance.Parent = game.Workspace
            
            -- Respawn w pƒôtli
            task.spawn(function()
                while partInstance do
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        partInstance.Position = player.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0)
                    end
                    wait(0.1) -- Respawn co 0.1 sekundy
                end
            end)
            
            Fluent:Notify({
                Title = "‚úÖ Sukces",
                Content = "Part respawnuje siƒô na Twojej pozycji!",
                Duration = 4
            })
        else
            Fluent:Notify({
                Title = "‚ùå B≈ÇƒÖd",
                Content = "Nie znaleziono Twojej postaci!",
                Duration = 4
            })
        end
    end
})

-- Konfiguracja mened≈ºer√≥w zapisu i interfejsu
SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)

SaveManager:IgnoreThemeSettings()
SaveManager:SetFolder("Dziewczynkomator")
InterfaceManager:SetFolder("Dziewczynkomator")

InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)

SaveManager:LoadAutoloadConfig()

-- Wyb√≥r domy≈õlnej zak≈Çadki
Window:SelectTab(1)

-- Powiadomienie ko≈Ñcowe
Fluent:Notify({
    Title = "Dziewczynkomator",
    Content = "Skrypt pomy≈õlnie za≈Çadowany!",
    Duration = 8
})

-- Czyszczenie Partu przy odrodzeniu postaci
game.Players.LocalPlayer.CharacterAdded:Connect(function(newCharacter)
    if partInstance then
        partInstance:Destroy()
        partInstance = nil
    end
end)
