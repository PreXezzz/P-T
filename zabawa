-- ≈Åadowanie Fluent i dodatk√≥w
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

-- Tworzenie okna
local Window = Fluent:CreateWindow({
    Title = "Dziewczynkomator",
    SubTitle = "by PreXezzz",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

-- Tworzenie zak≈Çadki
local Tabs = {
    Main = Window:AddTab({ Title = "G≈Ç√≥wne", Icon = "home" }),
    Settings = Window:AddTab({ Title = "Ustawienia", Icon = "settings" })
}

-- Powiadomienie przy uruchomieniu
Fluent:Notify({
    Title = "üöÄ Skrypt Za≈Çadowany",
    Content = "Witaj w Dziewczynkomatorze by PreXezzz!",
    Duration = 8
})

-- Przycisk teleportacji wszystkich graczy do mnie
Tabs.Main:AddButton({
    Title = "Teleportuj Wszystkich do Mnie üåç",
    Description = "Teleportuje wszystkich graczy do Twojej pozycji",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local rootPart = character:WaitForChild("HumanoidRootPart")
        
        if rootPart then
            for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                if otherPlayer ~= player and otherPlayer.Character then
                    local targetRoot = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if targetRoot then
                        targetRoot.CFrame = rootPart.CFrame + Vector3.new(math.random(-5, 5), 0, math.random(-5, 5))
                    end
                end
            end
            Fluent:Notify({
                Title = "‚úÖ Sukces",
                Content = "Wszyscy gracze teleportowani do Ciebie!",
                Duration = 4
            })
        else
            Fluent:Notify({
                Title = "‚ùå B≈ÇƒÖd",
                Content = "Nie znaleziono Twojej postaci!",
                Duration = 4
            })
        end
    end
})

-- Przycisk dodajƒÖcy napis "YouTuberüî¥" nad g≈ÇowƒÖ
Tabs.Main:AddButton({
    Title = "Dodaj Napis YouTuberüî¥ üìπ",
    Description = "Dodaje napis nad TwojƒÖ g≈ÇowƒÖ widoczny dla wszystkich",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local head = character:WaitForChild("Head")
        
        -- Usuwanie istniejƒÖcego napisu, je≈õli istnieje
        local existingLabel = character:FindFirstChild("YouTuberLabel")
        if existingLabel then
            existingLabel:Destroy()
        end
        
        -- Tworzenie napisu
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "YouTuberLabel"
        billboard.Adornee = head
        billboard.Size = UDim2.new(0, 100, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = character
        
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, 0, 1, 0)
        label.BackgroundTransparency = 1
        label.Text = "YouTuberüî¥"
        label.TextColor3 = Color3.fromRGB(255, 0, 0)
        label.TextScaled = true
        label.Font = Enum.Font.SourceSansBold
        label.Parent = billboard
        
        Fluent:Notify({
            Title = "‚úÖ Sukces",
            Content = "Napis YouTuberüî¥ dodany nad TwojƒÖ g≈ÇowƒÖ!",
            Duration = 4
        })
    end
})

-- Konfiguracja mened≈ºer√≥w zapisu i interfejsu
SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)

SaveManager:IgnoreThemeSettings()
SaveManager:SetFolder("Dziewczynkomator")
InterfaceManager:SetFolder("Dziewczynkomator")

InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)

SaveManager:LoadAutoloadConfig()

-- Wyb√≥r domy≈õlnej zak≈Çadki
Window:SelectTab(1)

-- Powiadomienie ko≈Ñcowe
Fluent:Notify({
    Title = "Dziewczynkomator",
    Content = "Skrypt pomy≈õlnie za≈Çadowany!",
    Duration = 8
})
